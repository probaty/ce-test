{
  "name": "dpg.aldar.ui.map-filters",
  "description": "A base template for building a shareable web components library with Vite + Svelte",
  "version": "0.0.0",
  "type": "module",
  "module": "dist/lib/dpg.aldar.ui.header.js",
  "main": "dist/lib/dpg.aldar.ui.header.umd.js",
  "scripts": {
    "start": "npm run dev -s",
    "dev": "vite --config vite.demo.config.ts",
    "build": "vite build",
    "build:demo": "vite build --config vite.demo.config.ts",
    "preview": "vite preview --config vite.demo.config.ts",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepublishOnly": "npm run build -s"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@tsconfig/svelte": "^4.0.1",
    "sass": "^1.60.0",
    "svelte": "^3.57.0",
    "svelte-check": "^3.1.4",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vite-plugin-filter-replace": "^0.1.11"
  },
  "files": [
    "dist/lib"
  ]
}
